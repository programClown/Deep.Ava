<Application
    x:Class="Deep.Ava.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mic="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
    xmlns:semi="https://irihi.tech/semi"
    xmlns:u-semi="https://irihi.tech/ursa/themes/semi"
    RequestedThemeVariant="Default">
    <!--  "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options.  -->

    <Application.Styles>
        <semi:SemiTheme Locale="zh-CN" />
        <semi:SemiPopupAnimations />
        <u-semi:SemiTheme Locale="zh-CN" />
        <StyleInclude Source="avares://Semi.Avalonia.DataGrid/Index.axaml" />
        <StyleInclude Source="avares://Deep.Ava/Styles/Index.axaml" />
        <StyleInclude Source="avares://Deep.Controls/Styles/Style.axaml" />

        <mic:MaterialIconStyles />
    </Application.Styles>
</Application>